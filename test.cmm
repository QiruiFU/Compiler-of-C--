int add(int temp[1000], int n){
    int i = 0;
    int ansadd = 0;
    while(i<n){
        ansadd = ansadd + temp[i];
        i = i+1;
    }
    return ansadd;
}

int main(){
    int a[100];
    int j=0;
    int tot = 0;
    while(j<100){
        a[j] = j+1;
        j=j+1;
    }
    tot = add(a, 100);
    write(tot);
    return 0;
}